@model FacturationWebSite.Models.InvoiceViewModel
@{
    ViewData["Title"] = "Print";
}

@await Html.PartialAsync("Invoice.cshtml", Model, this.ViewData)

@section Scripts {
	<script language="JavaScript" type="text/JavaScript">
		$(document).ready(function () {
			window.print();
			window.close();
		});
	</script>
}
